<div class="row repeater-item box box-primary" data-name="education">
  <div class="box-header with-border">
    <h3 class="box-title">Educaci√≥n</h3>
    <div class="box-tools pull-right hide">
      <button class="btn btn-danger btn-xs btn-box-tool remove-row"><i class="glyphicon glyphicon-remove"></i></button>
    </div><!-- /.box-tools -->
  </div><!-- /.box-header -->
  <div class="box-body">
    <div class="form-group col-md-6">
      <label for="course-degree" class="control-label">Nombre del curso</label>
      <input type="text" class="form-control" id="course-degree" name="education[0][degree]">
    </div>
    <div class="form-group col-md-6">
      <label for="school-name" class="control-label">Nombre del instituto</label>
      <input type="text" class="form-control" id="school-name" name="education[0][school]">
    </div>
    <div class="form-group col-md-6">
      <label for="start-date" class="control-label">Fecha de inicio</label>
      <input type="text" class="form-control date" id="start-date" name="education[0][start_date]">
    </div>
    <div class="form-group col-md-6">
      <label for="end-date" class="control-label">Fecha final</label>
      <input type="text" class="form-control date" id="end-date" name="education[0][end_date]">
    </div>
    <div class="form-group col-md-12">
      <label for="other-info" class="control-label">Otras informaciones</label>
      <textarea id="other-info" class="form-control" name="education[0][other_info]"></textarea>
    </div>
  </div>
</div>